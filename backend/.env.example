# ========== 服务器配置 ==========
PORT=3002
NODE_ENV=development

# ========== LLM 供应商配置 ==========
# 可选值: tongyi, deepseek
LLM_PROVIDER=tongyi

# ========== 通义千问配置 ==========
# 从阿里云百炼平台获取: https://bailian.console.aliyun.com/
TONGYI_API_KEY=sk-xxxxxxxxxxxxxx
TONGYI_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
TONGYI_MODEL=qwen-plus

# ========== DeepSeek 配置 ==========
# 从 DeepSeek 平台获取: https://platform.deepseek.com/
DEEPSEEK_API_KEY=sk-xxxxxxxxxxxxxx
DEEPSESK_BASE_URL=https://api.deepseek.com/v1
DEEPSEEK_MODEL=deepseek-chat

# ========== LLM 调用参数 ==========
LLM_TEMPERATURE=0.8
LLM_MAX_TOKENS=1000

# ========== 限流配置 ==========
RATE_LIMIT_PER_MINUTE=6

# ========== 缓存配置 ==========
CACHE_TTL=600
